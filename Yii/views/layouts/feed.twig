{% extends 'views/layouts/main.twig' %}
{% block content_wrap %}
    <div class="col-md-8">
        <div class="row">
            {% block content %}{% endblock %}
        </div><!-- /.content -->
        {% block pagination %}
            {{ void(this.widget('PaginationWidget')) }}
        {% endblock %}
    </div>
    <div class="col-md-4 sidebar">
        {{ void(this.widget('SidebarWidget')) }}
    </div><!-- /.sidebar -->
{% endblock %}